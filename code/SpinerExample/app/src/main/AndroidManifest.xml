<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.karomo.spinerexample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SimpleSpinnerActivity"
            android:label="@string/simple_spinner_activity"></activity>
        <activity
            android:name=".StoreSelectedItemActivity"
            android:label="@string/store_selected_pos"></activity>
        <activity
            android:name=".CustomAdapterSpinnerActivity"
            android:label="@string/custom_adapter_spinner"></activity>
        <activity
            android:name=".SQLiteDataSpinnerActivity"
            android:label="@string/sqlite_data_spinner_activity"></activity>
        <activity
            android:name=".MysqlDataSpinnerActivity"
            android:label="@string/mysql_data_spinner_activity"></activity>
    </application>

</manifest>
